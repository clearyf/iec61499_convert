chan oc_INIT_ZONEID;
chan oc_SC_INIT;
chan oc_SC_SEND;
chan oc_CYC_START;
chan oc_CYC_STOP;
chan oc_PENDING;
chan oc_INITO;
chan oc_MASTER;
chan oc_STATE;
bool isMasterIn;
int[0,4294967296] moduleIdIn;
int[0,4294967296] state;
bool reqPending;
int[0,4294967296] moduleId;
bool isMaster;
void algUnreg()
{
	state = 1;
	SC_INIT = IND_SEGID;
	reqPending = true;
	if (isMaster = true)
	{
		isMaster = false;
	}
	else
	{
		MASTER = false;
	}
}
void algReg()
{
	state = 2;
	reqPending = false;
	if (moduleId = 0 || moduleId = moduleIdIn)
	{
		moduleId = moduleIdIn;
		if (isMaster != isMasterIn)
		{
			isMaster = isMasterIn;
		}
		else
		{
			MASTER = false;
		}
	}
	else
	{
		MASTER = false;
		INITO = false;
	}
	SC_RECV = false;
}
void algZoneId()
{
	zoneIdAvailable = true;
}
void algInitZoneId()
{
	if (zoneIdAvailable)
	{
		INIT_ZONEID = false;
	}
}
